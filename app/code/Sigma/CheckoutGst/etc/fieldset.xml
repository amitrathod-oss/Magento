<?xml version="1.0"?>
<!--
/**
 * Sigma CheckoutGst Field Set Mapping
 * Maps custom field from quote to order
 *
 * @category  Sigma
 * @package   Sigma_CheckoutGst
 */
-->
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Sales:etc/fieldset.xsd">
    
    <fieldset name="sales_convert_quote">
        <field name="company_gst_no">
            <aspect name="to_order"/>
        </field>
    </fieldset>
    
</config>
